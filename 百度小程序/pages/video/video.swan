<view class="wrap">
    <view class="section">
        <video id="myde" style="width: 100%;" src="{{src}}" controls bindplay="play" bindpause="pause" bindfullscreenchange="fullscreen" bindended="ended" autoplay="{{autoplay}}" muted="{{muted}}"></video>
    </view>
    <view class="btn-area">
        <button bindtap="next" type="primary">切换视频地址</button>
    </view>
    <view class="btn-area">
        <button bindtap="setmuted" type="primary">设置静音</button>
    </view>
</view>
